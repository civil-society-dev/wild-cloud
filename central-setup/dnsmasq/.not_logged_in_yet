# PRESET_NET_STATIC_IP="192.168.8.50"
# PRESET_NET_STATIC_MASK="255.255.255.0"
# PRESET_NET_STATIC_GATEWAY="192.168.8.1"
# PRESET_NET_STATIC_DNS="1.1.1.1"

